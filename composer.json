{
  "name": "nettoweb/laravel-cms-store",
  "description": "Online store for Nettoweb content management system",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Netto\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Alexander Sidorenkov",
      "email": "contact@nettoweb.ru"
    }
  ],
  "require": {
      "nettoweb/laravel-cms": "^1.2",
      "nettoweb/laravel-cms-currency": "^1.2"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.5"
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Netto\\StoreServiceProvider"
      ]
    }
  }
}
